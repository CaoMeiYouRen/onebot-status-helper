{
  "name": "onebot-status-helper",
  "version": "0.1.0",
  "description": "转换 onebot 的 /get_status 接口的状态码",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "release": "semantic-release",
    "lint": "cross-env NODE_ENV=production eslint . --fix",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdown",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:tsup": "cross-env NODE_ENV=development tsdown --watch",
    "start": "node ./dist/index.mjs",
    "start:tsx": "cross-env NODE_ENV=production tsx src/index.ts",
    "start:benchmarks": "cross-env NODE_ENV=test BENCHMARKS_TEST=true node dist/index.mjs",
    "start:benchmarks:bun": "cross-env NODE_ENV=test BENCHMARKS_TEST=true bun dist/bun.mjs",
    "commit": "cz",
    "prepare": "husky",
    "test": "vitest run",
    "test:benchmarks": "tsx scripts/benchmarks.ts",
    "test:benchmarks:bun": "bun scripts/benchmarks.ts",
    "dev:vercel": "vercel dev",
    "deploy:vercel": "vercel",
    "dev:wrangler": "wrangler dev --env dev",
    "deploy:wrangler": "wrangler deploy",
    "dev:bun": "cross-env NODE_ENV=development bun --watch src/bun.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260131.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@types/autocannon": "^7.12.7",
    "@types/bun": "^1.3.8",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.9.2",
    "@vercel/nft": "^1.3.0",
    "autocannon": "^8.0.0",
    "commitizen": "^4.3.1",
    "commitlint": "^20.4.0",
    "commitlint-config-cmyr": "^1.0.0",
    "cross-env": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-cmyr": "^2.0.0",
    "debug": "^4.4.3",
    "eslint": "^9.39.2",
    "eslint-config-cmyr": "^2.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "octokit": "^5.0.5",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.3",
    "semantic-release-cmyr-config": "^1.0.1",
    "tsdown": "^0.20.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vercel": "^50.9.6",
    "vite": "^7.1.12",
    "vitest": "^4.0.18",
    "wrangler": "^4.61.1"
  },
  "config": {},
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "onebot",
    "docker",
    "typescript",
    "hono",
    "vercel",
    "cloudflare-workers"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/onebot-status-helper#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/onebot-status-helper.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/onebot-status-helper/issues"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "await-to-js": "^3.0.0",
    "better-bytes": "^1.2.6",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "hono": "^4.11.10",
    "isomorphic-unfetch": "^4.0.2",
    "lodash-es": "^4.17.23",
    "tslib": "^2.8.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "compare-func": "^2.0.0",
      "undici@>=4.5.0 <5.28.5": ">=5.28.5",
      "esbuild@<=0.24.2": ">=0.25.0",
      "undici@<5.29.0": ">=5.29.0",
      "tar@=7.5.1": ">=7.5.2",
      "tmp@<=0.2.3": ">=0.2.4",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "glob@>=11.0.0 <11.1.0": ">=11.1.0"
    }
  },
  "type": "module"
}